			<MaxHitPoints>250</MaxHitPoints>
			<WorkToMake>10200</WorkToMake>
			<Mass>6.0</Mass>
			<Insulation_Cold>3</Insulation_Cold>
			<Insulation_Heat>3</Insulation_Heat>
			<EquipDelay>1.2</EquipDelay>
		</statBases>
		<apparel>
			<shellRenderedBehindHead>true</shellRenderedBehindHead>
			<bodyPartGroups>
				<li>Hands</li>
				<li>Feet</li>
				<li>Neck</li>
				<li>Legs</li>
				<li>Arms</li>
				<li>Torso</li>
				<li>Waist</li>
				<li>Shoulders</li>
			</bodyPartGroups>
			<layers>
				<li>Shell</li>
			</layers>
			<tags>
				<li>FCP_Power_Armor</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
			<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
			<canBeGeneratedToSatisfyVacuumResistance>false</canBeGeneratedToSatisfyVacuumResistance>
			<canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
			<canBeDesiredForIdeo>false</canBeDesiredForIdeo>
			<useDeflectMetalEffect>true</useDeflectMetalEffect>
			<developmentalStageFilter>Adult</developmentalStageFilter>
		</apparel>
		<tradeTags>
			<li>FCP_Power_Armor_Basic</li>
		</tradeTags>
		<building />
		<comps>
			<li Class="FCP.Core.CompProperties_LabelColored">
				<rarity>Rare</rarity>
			</li>
			<li Class="RangerRick_PowerArmor.CompProperties_RepairableAtStation">
